              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/Memory.*/"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "decgbytes"
                }
              ]
            },
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/CPU.*/"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                }
              ]
            }
          ]
        },
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 17}
