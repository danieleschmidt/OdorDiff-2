        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{pod=~\"odordiff-api.*\"}[5m])",
            "refId": "A",
            "legendFormat": "CPU Usage - {{pod}}"
          },
          {
            "expr": "container_memory_working_set_bytes{pod=~\"odordiff-api.*\"} / 1024 / 1024 / 1024",
            "refId": "B",
            "legendFormat": "Memory Usage - {{pod}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
