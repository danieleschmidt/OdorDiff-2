            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"odordiff-api\"}[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"odordiff-api\"}[5m]))",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
